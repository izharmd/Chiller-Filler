<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bws.chillerfiller">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".util.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo_chiller_filler"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_chiller_filler"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.MuscleFood">
        <activity android:name="com.bws.chillerfiller.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.bws.chillerfiller.signup.SignUpActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.login.LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.itemcategory.ItemCategotyActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.itemcategory.subcategory.SubCategoryActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.itemcategory.productlist.ProductListActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.itemcategory.cartlist.CartListActivity"
            android:screenOrientation="portrait"></activity>


        <activity
            android:name="com.bws.chillerfiller.delivery.choosedeliveryaddress.ChooseDeliveryAddressActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.delivery.deliveryoption.DeliveryOptionActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.delivery.AddNewAddressActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.payment.PaymentActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.MyProfileActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.orders.OrderActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.itemcategory.basket.BasketsActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.bws.chillerfiller.orders.SearchOrderActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.bws.chillerfiller.enotes.EnotesActivity"
            android:screenOrientation="portrait"></activity>

        <activity android:name=".login.LoginActivityTest"
            android:screenOrientation="portrait"></activity>

    </application>

</manifest>